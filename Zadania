1)
cat numbers.txt | sort -r | uniq
2)
cat cars.txt | cut -d " " -f 3 | sort -r | head -n 1
3)
sed -n 123p file.txt
4)
sed -n '10,20p;21q' file.txt
5)
egrep -o 'cz|sz' file.txt | wc -l
6)
cat file.txt | tr ąĄćĆęĘłŁńŃóÓśŚźŹżŻ X
7)
cat file.txt | nl | sort -r
8)
cat file.txt | tr -d '[:punct:]' | wc -w

